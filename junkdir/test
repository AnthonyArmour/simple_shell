#include "shell.h"
int main(void)
{
	int y = 0;
	int id = fork();
	char buf[1024];
	pid_t my_pid;

	my_pid = getpid();
	if (my_pid != 0)
	{
		while(wait(NULL) != -1)
			;
		write(1, (char *)my_pid, strlen((char *)my_pid))
	}
	if (id == 0)
		printf("Prompt $");
	else
		y = read(STDIN_FILENO, buf, 1024);
	if (y != 0)
		write(STDOUT_FILENO, buf, y);
	return (0);
}
